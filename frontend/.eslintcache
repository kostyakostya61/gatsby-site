}>\n        <div className={style.shade}>\n          <div className={style.content}>\n            <div className={style.navigation}>\n              <div className={style.brand}>\n                <div>\n                  <img src={image} />\n                </div>\n                <div>\n                  <p>CLOUDBUDGET</p>\n                </div>\n              </div>\n\n              <div className={style.navbar}>\n                <div className={style.link}>{linkComponents}</div>\n              </div>\n\n              <div className={style.identification}>\n                <div className={style.btn}>\n                  <button onClick={logoutHandler}>LOG OUT</button>\n                </div>\n              </div>\n            </div>\n\n            <div className={style.header}>\n              <h3>CLOUDBUDGET</h3>\n\n              <div className={style.greeting}>\n                {!userValue.name ? (\n                  <Spinner intent=\"primary\" />\n                ) : (\n                  <p>Hello {userValue.name}!</p>\n                )}\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <Description />\n    </>\n  );\n}\n\nexport default AuthPage;\n","/home/user/projects/gatsby-app/gatsby-site/frontend/src/pages/change-data-page/change-data-page.js",["101","102"],"import React, { useCallback, useContext, useEffect, useState } from 'react';\nimport style from './change-data-page.module.scss';\nimport image from '../../images/image.png';\n\nimport { AuthContext } from '../../components/context/auth-context';\nimport { useHistory } from 'react-router-dom';\nimport { UserContext } from '../../components/context/user-context';\n\nimport { Spinner } from '@blueprintjs/core';\n\nfunction AuthPage() {\n  const [userValue, setUserValue] = useState({});\n  const auth = useContext(AuthContext);\n  const user = useContext(UserContext);\n  // console.log(user.data);\n  let history = useHistory();\n  const logoutHandler = (event) 